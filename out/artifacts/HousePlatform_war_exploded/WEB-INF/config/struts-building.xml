<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="building" namespace="/building" extends="base-package">
        <action name="*" class="buildingAction" method="{1}">
            <result name="totalbuilding">/agency/totalbuilding.jsp</result>
            <result name="addbuildingpage">/agency/addbuilding.jsp</result>
            <result name="addbuilding" type="json"></result>
            <result name="allBuilding" type="json"></result>
            <result name="allAgencyBuilding" type="json"></result>
            <result name="updateStatus" type="json"></result>
            <result name="updateBuilding" type="json"></result>
            <result name="queryById" type="json"></result>
            <result name="allBuildingPrice" type="json"></result>
        </action>
    </package>
</struts>