<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="public-pack" namespace="/index" extends="base-package">
        <action name="*" class="publicLoginAction" method="{1}">
            <result name="index">/index/index.jsp</result>
            <result name="indexRegister">/index/indexRegister.jsp</result>
            <result name="indexlogin" >/index/indexlogin.jsp</result>
            <result name="login" type="json"></result>
            <result name="queryEmail" type="json"></result>
            <result name="queryName" type="json"></result>
            <result name="queryPhone" type="json"></result>
            <result name="waitaudit" type="json"></result>
            <result name="reg" type="json"></result>
            <result name="queryByPhone" type="json"></result>
            <result name="remove" type="redirect">/index/index</result>

            <result name="buildingspage">/index/buildings.jsp</result>
            <result name="housepage">/index/house.jsp</result>
            <result name="Informationpage">/index/Information.jsp</result>
            <result name="activitypage">/index/activity.jsp</result>
        </action>
    </package>
</struts>