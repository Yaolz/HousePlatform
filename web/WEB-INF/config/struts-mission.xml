<struts>
    <package name="mission_package" namespace="/mission" extends="base-package">
        <action name="*" class="missionAction" method="{1}">
            <result name="allmissionpage" >/agency/allmissionpage.jsp</result>
            <result name="addmission" type="json"></result>
            <result name="allmission" type="json"></result>
            <result name="updatequantity" type="json"></result>
            <result name="updatepercent" type="json"></result>
            <result name="updateStatus" type="json"></result>
            <result name="allEmployeeMission" type="json"></result>
            <result name="addMessage" type="json"></result>
            <result name="buildingsMessage" type="json"></result>
        </action>

    </package>

</struts>